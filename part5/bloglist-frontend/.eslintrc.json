{
  "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true,
    "cypress/globals": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "jest",
    "cypress"
  ],
  "rules": {
    "semi": 0,
    "no-console": 0
  }
}
